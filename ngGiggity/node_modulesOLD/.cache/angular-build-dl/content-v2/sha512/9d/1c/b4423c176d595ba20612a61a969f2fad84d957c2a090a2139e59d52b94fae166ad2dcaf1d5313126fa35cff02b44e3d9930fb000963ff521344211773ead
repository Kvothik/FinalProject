                SearchResultsComponent.prototype.ngOnInit = function () {
                    this.getLoggedUser();
                    if (this.currentRoute.snapshot.paramMap.get('skillName')) {
                        this.jobSkillName = this.currentRoute.snapshot.paramMap.get('skillName');
