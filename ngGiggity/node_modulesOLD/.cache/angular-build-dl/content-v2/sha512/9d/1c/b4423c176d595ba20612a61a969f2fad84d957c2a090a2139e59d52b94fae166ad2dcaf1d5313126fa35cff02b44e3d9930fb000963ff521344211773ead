                    this.sellerReviews = [];
                }
                AltProfileComponent.prototype.ngOnInit = function () {
                    var _this = this;
                    this.username = this.userSvc.profile;
                    this.userSvc.findUserByUsername(this.username).subscribe(function (data) {
                        _this.user = data;
                        _this.showByBidder(data.id);
                        _this.showHistory(data.id);
                        _this.allSellerReview();
                    }, function (err) { return console.error('Reload error in User Component'); });
                    this.getUserJobs();
                };
                AltProfileComponent.prototype.allSellerReview = function () {
                    var _this = this;
                    this.bookingSvc.findAllSellerReviews().subscribe(function (data) {
                        // tslint:disable-next-line: prefer-for-of
                        console.log('LOOOOOOOOOOOOOOOOOOOOOOK: ' + data.length);
                        // tslint:disable-next-line: prefer-for-of
                        for (var i = 0; i < data.length; i++) {
                            if (data[i].booking) {
                                if (data[i].booking.bid.bidder.id === _this.user.id) {
                                    _this.sellerReviews.push(data[i]);
                                }
                            }
                        }
                        if (_this.sellerReviews.length > 0) {
                            _this.averageSellerReview = 0;
                            // tslint:disable-next-line: prefer-for-of
                            for (var i = 0; i < _this.sellerReviews.length; i++) {
                                _this.averageSellerReview = _this.averageSellerReview + _this.sellerReviews[i].rating;
                            }
                            _this.averageSellerReview = _this.averageSellerReview / _this.sellerReviews.length;
                            _this.averageSellerReview = _this.averageSellerReview + ' / 5';
                        }
                        else {
                            _this.averageSellerReview = 'No reviews';
                        }
                    }, function (err) { return console.error('Reload error in Component'); });
                };
