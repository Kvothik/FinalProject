                    var _this = this;
                    this.userSvc.findJobsByUsername(this.username).subscribe(function (data) {
                        _this.userJobs = data;
                    }, function (err) { return console.error('Reload error in Component'); });
                };
                AltProfileComponent.prototype.showHistory = function (user) {
                    var _this = this;
                    this.bookingSvc.findAll(user).subscribe(function (data) {
                        _this.allBoookings = data;
                        _this.allBoookings.forEach(function (b) {
                            if (b.completeDate !== null) {
                                _this.transactionHistory.push(b);
                            }
                        });
                    }, function (err) { return console.error('Reload error in Component'); });
                };
