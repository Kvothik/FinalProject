                    var httpOptions = {
                        headers: new _angular_common_http__WEBPACK_IMPORTED_MODULE_2__["HttpHeaders"]({
                            Authorization: "Basic " + credentials,
                            'X-Requested-With': 'XMLHttpRequest'
